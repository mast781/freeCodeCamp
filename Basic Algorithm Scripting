// Convert Celsius to Fahrenheit
const convertToF = (celsius) => celsius * 9 / 5 + 32 

convertToF(30);

// Reverse a String
const reverseString = (str) => str.split("").reverse().join("");

reverseString("hello");

// Factorialize a Number
function factorialize(num) {
  let factorial = 1;
  for (let i = 1; i <= num; i++) factorial *= i;
  return factorial;
}

factorialize(5);

