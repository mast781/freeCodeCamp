// Iterate with JavaScript While Loops

const myArray = [];

let i = 5;
while (i >= 0) {
  myArray.push(i);
  i--;
}

// Iterate with JavaScript For Loops

const myArray = [];

for (let i = 1; i <= 5; i++) {
  myArray.push(i);
}

// Iterate Odd Numbers With a For Loop

const myArray = [];

for (let i = 1; i < 11; i += 2) {
  myArray.push(i);
}
